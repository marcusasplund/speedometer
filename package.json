  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/marcusasplund/speedometer.git"
  },
  "bugs": {
    "url": "https://github.com/marcusasplund/speedometer/issues"
  },
  "homepage": "https://github.com/marcusasplund/speedometer#readme",
  "devDependencies": {
    "@babel/core": "^7.1.5",
    "@babel/preset-env": "7.1.5",
    "babel-eslint": "^10.0.1",
    "babel-preset-env": "^1.7.0",
    "parcel-bundler": "^1.10.3",
    "parcel-plugin-sw-cache": "^0.2.6",
    "standard": "*"
  },
  "cache": {
    "inDev": true,
    "swDest": "service-worker.js"
  },
  "standard": {
    "parser": "babel-eslint",
    "ignore": [
      "dist/",
      "build/"
