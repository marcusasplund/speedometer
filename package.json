{
  "name": "speedometer",
  "version": "0.0.3",
  "author": "Marcus Asplund <marcus@greatname.se> (@marcusasplund)",
  "license": "MIT",
  "scripts": {
    "start": "standard && parcel src/index.html -p 3000 --open",
    "build": "rm -rf /release && standard && parcel build src/index.html -d build --public-url ./"
  },
  "main": "src/app/index.js",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/marcusasplund/speedometer.git"
  },
  "bugs": {
    "url": "https://github.com/marcusasplund/speedometer/issues"
  },
  "homepage": "https://github.com/marcusasplund/speedometer#readme",
  "devDependencies": {
    "@babel/core": "^7.6.3",
    "@babel/preset-env": "7.6.3",
    "babel-eslint": "^10.0.3",
    "babel-preset-env": "^1.7.0",
    "parcel-bundler": "^1.12.3",
    "parcel-plugin-sw-cache": "^0.3.1",
    "standard": "*"
  },
  "cache": {
    "inDev": true,
    "swDest": "service-worker.js"
  },
  "standard": {
    "parser": "babel-eslint",
    "ignore": [
      "dist/",
      "build/"
    ]
  },
  "dependencies": {}
}
